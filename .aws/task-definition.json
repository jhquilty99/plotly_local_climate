{  
   "memory":"1024",
   "networkMode":"awsvpc",
   "cpu":"512",
   "family":"climate-change-app",
   "requiresCompatibilities": [
      "FARGATE"
   ], 
   "executionRoleArn": "arn:aws:iam::485489709123:role/ECSTaskRole",
   "containerDefinitions":[  
      {  
         "portMappings":[  
            {  
               "hostPort":5000,
               "containerPort":5000,
               "protocol":"tcp"
            }
         ],
         "essential":true,
         "name":"climate-web",
         "secrets":[  
            {
               "name": "OPEN_METEO_API_KEY",
               "valueFrom": "arn:aws:secretsmanager:us-east-1:485489709123:secret:OPEN_METEO_API_KEY-FfLvJo"
            }
         ],
         "image":"personal_website_images/climate-change:latest",
         "logConfiguration":{  
            "logDriver":"awslogs",
            "options":{  
               "awslogs-group":"climate-web",
               "awslogs-region":"us-east-1",
               "awslogs-stream-prefix":"ecs"
            }
         },
         "memoryReservation":1024
      }
   ]
}