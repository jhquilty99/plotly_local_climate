{  
   "memory":"1024",
   "networkMode":"awsvpc",
   "cpu":"512",
   "family":"climate-change-app",
   "requiresCompatibilities": [
      "FARGATE"
   ], 
   "containerDefinitions":[  
      {  
         "portMappings":[  
            {  
               "hostPort":5000,
               "containerPort":5000,
               "protocol":"tcp"
            }
         ],
         "essential":true,
         "name":"climate-web",
         "environment":[  

         ],
         "image":"personal_website_images/climate-change:latest",
         "logConfiguration":{  
            "logDriver":"awslogs",
            "options":{  
               "awslogs-group":"/ecs/climate-web",
               "awslogs-region":"us-east-1",
               "awslogs-stream-prefix":"ecs"
            }
         },
         "memoryReservation":1024
      }
   ]
}